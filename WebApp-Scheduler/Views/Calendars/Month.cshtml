<div class="single-ele">
    <table class="table-st">
        <caption class="caption-st">
            <b>@ViewBag.Month / @ViewBag.Year</b>
        </caption>
        <thead class="thread-st">
            <tr class="tr-st">
                <th class="th-first" abbr="Sunday" title="Sunday"> Sunday </th>
                <th abbr="Monday" class="th-first" title="Monday"> Monday </th>
                <th abbr="Tuesday" title="Tuesday" class="th-first"> Tuesday </th>
                <th abbr="Wednesday" title="Wednesday" class="th-first"> Wednesday </th>
                <th abbr="Thursday" title="Thursday" class="th-first"> Thursday </th>
                <th abbr="Friday" title="Friday" class="th-first"> Friday </th>
                <th abbr="Saturday" title="Saturday" class="th-first"> Saturday </th>
            </tr>
        </thead>
        <tbody class="tbody-st">

            @for (var i = 1; i < @ViewBag.TotalDays; i += 7)
            {
                <tr class="tr-st">
                    @for (var j = 0; j < 7; j++)
                    {
                        if (ViewBag.StartingDate <= (i + j))
                        {
                            <td class="not-selected">@(i + j)</td>
                        }
                        else
                        {
                            <td>@i</td>

                        }
                    }
                </tr>
            }

        </tbody>
    </table>
</div>


